<h1>List of articles:</h1>
<ul>
  {{#each articles}}
    <li>{{this.title}}</li>
      <ul>
        <li><a href="http://{{this.url}}">{{this.url}}</a></li>
      </ul>
  {{/each}}
</ul>

<div class="sit">
	<li><a href="/siteAdmin" id="siteA">Contact Site Administrator</a></li>
</div>

<div>
	<button class="back-btn" onclick="history.back()">back</button>
</div>

<div>
	<button class="logoff-btn" onclick=logOut()>Logout</button>
</div>

<script>
		var logOut = function (){
			window.location.href="/logout";
	};
</script>


